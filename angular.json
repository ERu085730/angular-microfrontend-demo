            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "page1:build:production",
              "extraWebpackConfig": "projects/page1/webpack.prod.config.js"
            },
            "development": {
